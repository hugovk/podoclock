#include "PodOClock.loc"
#include <appinfo.rh>
#include <PodOClock.rsg>

CHARACTER_SET UTF8

UID2 KUidAppRegistrationResourceFile
UID3 0xA89FB98E

RESOURCE APP_REGISTRATION_INFO
	{
	app_file="PodOClock";  // filename of application binary (minus extension)
	localisable_resource_file = "\\resource\\apps\\PodOClock";
	localisable_resource_id = R_PODOCLOCK_LOCALISABLE_APP_INFO;

	embeddability=KAppNotEmbeddable;
	newfile=KAppDoesNotSupportNewFile;
	}

// End of file
